{
    "manifest_version": 2,
    "name": "Mark Reading",
    "description": "This extension helps you to organize web articles and record your reading status",
    "version": "1.0",
    "icons": {
        "16": "img/markreading16.png",
        "48": "img/markreading48.png",
        "128": "img/markreading128.png"
    },
    "browser_action": {
        "default_icon": "img/markreading.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
